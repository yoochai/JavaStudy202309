package ex06;

import java.util.function.Function;

//Function<T, R>	T -> R	R apply(T t)





public class Lambda9_2 {	
	public static void main(String[] args) {
		Function<Integer, Integer> f1 =
				(Integer t)-> {
				int sum = 0;
				int t1[] = null;
				for(int i : t1)
					sum+=i;
				return sum;
			};
		int[] t = {1,2,3,4,5};
		int result = f1.apply(null);
		System.out.println(result);

	}
}
