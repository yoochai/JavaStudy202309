package ex07;

public class MethodReferenceExam {

	public static void main(String[] args) {
		Person person = new Person();
		
		person.action(new Calculable() {
			
			@Override
			public double calc(double x, double y) {
				return x+y;
			}
		});

		person.action((double x, double y) -> x+y;
		
	}

}
